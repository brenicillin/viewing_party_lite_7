<h1> Viewing Party </h1>

<% if current_user %>
  <h3> Welcome, <%= current_user.name %>! </h3>
  <%= button_to "Log Out", session_path(current_user.id), {method: "delete"} %>
    <% elsif !current_user %>
  <%= button_to "Register", "/register", {method: "get"}  %>
  <%= button_to "Log In", new_session_path, {method: "get"} %>
<% end %>


<% if current_user %>
  <ul id="current-users">
    <h3> Current Users </h3>
    <% @users.each do |user| %>
      <li><%= user.email %>
      <%# <li><%= link_to "#{user.name}", user_path(user)%></li>
    <% end %>
  </ul>
<% end %>